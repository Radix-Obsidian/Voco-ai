# Voco Cognitive Engine - Production Environment Variables
# Copy this file to .env and fill in your actual API keys

# Required: AI Model API Key
ANTHROPIC_API_KEY=your_anthropic_key_here

# Required: Speech-to-Text
DEEPGRAM_API_KEY=your_deepgram_key_here

# Required: Text-to-Speech
CARTESIA_API_KEY=your_cartesia_key_here

# Required: Database & Auth
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Optional: Billing (Stripe)
STRIPE_SECRET_KEY=

# Optional: GitHub Integration
GITHUB_TOKEN=

# Optional: Web Search
TAVILY_API_KEY=

# Optional: Google AI
GOOGLE_API_KEY=

# Optional: TTS Voice ID (default is provided)
TTS_VOICE=79a125e8-cd45-4c13-8a67-188112f4dd22
